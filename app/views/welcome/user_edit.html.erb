<h1>Editing User</h1>

<div style="margin:50px;">
  <h3>Edit User</h3>

  <%= form_for @user do |f| %>
    <%= f.text_field :name, placeholder: "Name" %><br />
    <%= f.number_field :age, placeholder: "Age" %><br />
    <%= f.submit %>
  <% end %>
  <!-- <%=
  #link_to 'View All', users_path(@users)
  %> -->
</div>

